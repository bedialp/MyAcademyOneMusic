@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/UILayout/Index.cshtml";
}
<!-- ##### Hero Area Start ##### -->
@await Component.InvokeAsync("_DefaultBannerComponent")
<!-- ##### Hero Area End ##### -->
<!-- ##### Latest Albums Area Start ##### -->
@await Component.InvokeAsync("_DefaultAlbumComponent")
<!-- ##### Latest Albums Area End ##### -->
<!-- ##### Buy Now Area Start ##### -->
@await Component.InvokeAsync("_DefaultBuyNowAreaComponent")
<!-- ##### Buy Now Area End ##### -->
<!-- ##### Featured Artist Area Start ##### -->
@await Component.InvokeAsync("_DefaultFeaturedArtistComponent")
<!-- ##### Featured Artist Area End ##### -->
<!-- ##### Miscellaneous Area Start ##### -->
<section class="miscellaneous-area section-padding-100-0">
	<div class="container">
		<div class="row">
			<!-- ***** Weeks Top ***** -->
			@await Component.InvokeAsync("_DefaultLatestAlbumComponent")
			<!-- ***** New Hits Songs ***** -->
			@await Component.InvokeAsync("_DefaultHitSongComponent")
			<!-- ***** Popular Artists ***** -->
			@await Component.InvokeAsync("_DefaultPopularArtistComponent")
		</div>
	</div>
</section>
<!-- ##### Miscellaneous Area End ##### -->
<!-- ##### Contact Area Start ##### -->
@await Component.InvokeAsync("_DefaultMessageComponent")
<!-- ##### Contact Area End ##### -->
